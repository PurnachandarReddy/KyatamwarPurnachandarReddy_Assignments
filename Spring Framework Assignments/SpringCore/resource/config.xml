<?xml version="1.0" encoding="UTF-8"?>

<beans xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
http://www.springframework.org/schema/context 
http://www.springframework.org/schema/context/spring-context-3.0.xsd" 
xmlns:context="http://www.springframework.org/schema/context" 
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://www.springframework.org/schema/beans">

<context:annotation-config/>


<bean class="com.annotations.resource.Company" name="mycompany">

<property name="name" value="Capgemini"/>

<property name="location" value="Bangalore"/>

</bean>


<bean class="com.annotations.resource.Employee" name="myemployee">

<property name="id" value="101"/>

<property name="name" value="KPCReddy"/>

</bean>

</beans>